      "source": [
        "# текст запроса\n",
        "query_5_2 = f'''\n",
        "select \n",
        "a.name area,\n",
        "(select count(*) from public.vacancies v where v.area_id=a.id) vacancies,\n",
        "(select count(*) from public.employers e where e.area=a.id) employers \n",
